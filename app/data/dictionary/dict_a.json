                    "examples": [
                      {
                        "tp": "aan a'e.",
                        "variation": "aan a'é nhẽ.", 
                        "pt": "Digo não, digo que não.",
                        "src": "(VLB, II, 99)"
                      },
                      {
                        "tp": "aan ipó, aan ipó biã",
                        "pt": "não deve ser, não será assim",
                        "src": "(VLB, II, 47)"
                      },
