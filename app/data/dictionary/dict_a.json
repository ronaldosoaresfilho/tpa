            "word": "aan",
            "variation": "aã",
            "class": "partícula",
            "meaning": [
              {
                "cases": [
                  {
                    "pt": "não (de homem ou mulher)",
                    "examples": [
                      {
                        "tp": "Aan, nd'ereîtyki xóne.",
                        "pt": "Não, não os derrotarás.",
                        "src": "(Anch., Teatro, 136)"
                      },
                      {
                        "tp": "Aã! Xe potaba nde!",
                        "pt": "Não! Meu quinhão é tu!",
                        "src": "(Anch., Teatro, 76)"
                      }
                    ]
                  }
                ]
              }
            ],
            "context": [
              {
                "cases": [
